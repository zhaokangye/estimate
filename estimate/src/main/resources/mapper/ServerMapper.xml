<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kang.estimate.module.management.dao.ServerMapper">

    <select id="findServerByUserid" resultType="com.kang.estimate.module.management.entity.Server">
        select *
        from server
        where userId=#{userId}
    </select>
</mapper>


